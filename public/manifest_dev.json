{
  "name": "FieldDB unstable",
  "version": "1.21.2",
  "description": "__MSG_application_description__",
  "default_locale": "en",
  
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage",
    "https://www.fieldlinguist.com/*",
    
    "https://auth.fieldlinguist.com/*",
    "https://audio.fieldlinguist.com/*",
    "https://lexicon.fieldlinguist.com/*",
    "https://corpus.fieldlinguist.com/*",
    "https://activity.fieldlinguist.com/*",
    "https://widget.fieldlinguist.com/*",
    
    "https://ifield.iriscouch.com/*",
    
    "https://authdev.fieldlinguist.com:3183/*",
    "https://audiodev.fieldlinguist.com:3184/*",
    "https://lexicondev.fieldlinguist.com:3185/*",
    "https://corpusdev.fieldlinguist.com:3186/*",
    "https://activitydev.fieldlinguist.com:3187/*",
    "https://widgetdev.fieldlinguist.com:3188/*",
    
    "https://ifielddevs.iriscouch.com/*",
    
    "https://www.youtube.com/*"
  ],
   "icons": { 
    "16": "icon16_dev.png",
    "48": "icon48_dev.png",
    "128": "icon128_dev.png" 
  },
  "manifest_version": 2,
  "content_security_policy": "default-src 'self' https://authdev.fieldlinguist.com:3183/  https://si0.twimg.com https://ssl.google-analytics.com/ https://www.youtube.com/ ; script-src 'self' https://authdev.fieldlinguist.com:3183/ https://ssl.google-analytics.com/ ; connect-src  https://www.youtube.com/  https://authdev.fieldlinguist.com:3183/ https://ifielddevs.iriscouch.com/ https://ifielddevs.iriscouch.com:443/ 'self'; object-src 'self';"
 
}
