{

  
    "name": "__MSG_iField beta__",
     "version": "1.13.1",
    "description": "__MSG_An on/offline fieldlinguistics database app which adapts to its user's I-Language.__",
    "default_locale": "en",
  
  "app": {
    "launch": {
      "local_path": "dashboard.html"
    }
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage",
    "https://ifield.fieldlinguist.com/*",
    "https://ilanguage.iriscouch.com/*",
    "https://localhost:3001/*",
    "http://localhost:5984/*"
  ],
   "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  }
}
