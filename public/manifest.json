{
  "name": "iField",
  "version": "1.8.4",
  "description": "A Fieldlinguistics Database which adapts to its user's I-Language.",
  "app": {
    "launch": {
      "local_path": "dev.html"
    }
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage",
    "<all_urls>"
  ],
   "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  }
  
}
