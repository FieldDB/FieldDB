{
  "name": "iField beta",
  "version": "1.12.2",
  "description": "An on/offline fieldlinguistics database app which adapts to its user's I-Language.",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage",
    "https://ifield.fieldlinguist.com/*",
    "https://ilanguage.iriscouch.com/*"
  ],
   "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  }
}
