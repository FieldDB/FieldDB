{
  "name": "iField",
  "version": "1.11",
  "description": "An on/offline fieldlinguistics database app which adapts to its user's I-Language.",
  "app": {
    "launch": {
      "local_path": "dev.html"
    }
  },
  "offline_enabled": true,
  "permissions": [
    "unlimitedStorage",
    "https://ifield.fieldlinguist.com/",
    "http://ifield.fieldlinguist.com/",
    "https://localhost:3001/",
    "http://localhost:3001/",
    "https://ilanguage.iriscouch.com/",
    "http://ilanguage.iriscouch.com/"
  ],
   "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "manifest_version": 2
  
}
